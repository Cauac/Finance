<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="220.0" prefWidth="220.0" style="-fx-border-radius: 10; -fx-border-color: #000000;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="finance.controller.AccountController">
   <children>
      <Label fx:id="title" alignment="CENTER" prefHeight="20.0" prefWidth="220.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
       <Label text="Остаток:">
           <font>
               <Font size="16.0" />
           </font>
       </Label>
       <Label alignment="CENTER" text="0$">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
       </Label>
       <Label text="Тип:">
           <font>
               <Font size="16.0" />
           </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
       </Label>
       <Label alignment="CENTER" text="наличные">
         <padding>
            <Insets top="5.0" />
         </padding>
       </Label>
      <HBox spacing="5.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" text="Изменить" />
            <Button mnemonicParsing="false" onMouseReleased="#removeBtn" prefHeight="40.0" prefWidth="110.0" text="Удалить" />
         </children>
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets left="16.0" right="16.0" top="5.0" />
   </padding>
</VBox>
